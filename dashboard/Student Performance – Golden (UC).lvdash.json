{
  "datasets": [
    {
      "name": "ff2fa7f1",
      "displayName": "Tile 1 – KPI principal (promedio de nota)",
      "queryLines": [
        "SELECT\r\n",
        "  ROUND(AVG(Exam_Score), 1) AS avg_exam,\r\n",
        "  COUNT(*)                 AS total_students\r\n",
        "FROM catalog_dev.golden.fact_student_performance;"
      ]
    },
    {
      "name": "1ed75118",
      "displayName": "Tile 2 – Promedio por género",
      "queryLines": [
        "SELECT\r\n",
        "  Gender,\r\n",
        "  avg_exam,\r\n",
        "  n\r\n",
        "FROM catalog_dev.golden.agg_exam_by_gender;\r\n"
      ]
    },
    {
      "name": "5038c290",
      "displayName": "Tile 3 – Promedio por horas de estudio",
      "queryLines": [
        "SELECT\r\n",
        "  hours_bucket,\r\n",
        "  avg_exam,\r\n",
        "  n\r\n",
        "FROM catalog_dev.golden.agg_exam_by_hours_bucket\r\n",
        "ORDER BY CASE hours_bucket\r\n",
        "           WHEN '0-5' THEN 1\r\n",
        "           WHEN '6-10' THEN 2\r\n",
        "           WHEN '11-15' THEN 3\r\n",
        "           WHEN '16-20' THEN 4\r\n",
        "           ELSE 5\r\n",
        "         END;\r\n"
      ]
    },
    {
      "name": "0a5a20b4",
      "displayName": "Tile 4 – Promedio por asistencia",
      "queryLines": [
        "SELECT\r\n",
        "  attendance_bucket,\r\n",
        "  avg_exam,\r\n",
        "  n\r\n",
        "FROM catalog_dev.golden.agg_exam_by_attendance_bucket\r\n",
        "ORDER BY CASE attendance_bucket\r\n",
        "           WHEN '<50'   THEN 1\r\n",
        "           WHEN '50-75' THEN 2\r\n",
        "           WHEN '76-90' THEN 3\r\n",
        "           ELSE 4\r\n",
        "         END;"
      ]
    },
    {
      "name": "019b7808",
      "displayName": "Tile 5 – Motivación vs desempeño",
      "queryLines": [
        "SELECT\r\n",
        "  Motivation_Level,\r\n",
        "  avg_exam,\r\n",
        "  n\r\n",
        "FROM catalog_dev.golden.agg_exam_by_motivation\r\n",
        "ORDER BY avg_exam DESC;"
      ]
    },
    {
      "name": "323549ec",
      "displayName": "Tile 6 – School Type vs Income",
      "queryLines": [
        "SELECT\r\n",
        "  School_Type,\r\n",
        "  Family_Income,\r\n",
        "  ROUND(avg_exam, 1) AS avg_exam,\r\n",
        "  n\r\n",
        "FROM catalog_dev.golden.agg_exam_school_vs_income;"
      ]
    },
    {
      "name": "04e630f2",
      "displayName": "Tile 7 – Correlaciones",
      "queryLines": [
        "SELECT\r\n",
        "  feature,\r\n",
        "  corr\r\n",
        "FROM catalog_dev.golden.corr_with_exam\r\n",
        "ORDER BY corr DESC;"
      ]
    },
    {
      "name": "110a4b5d",
      "displayName": "Tile 8 – Dispersión horas vs nota",
      "queryLines": [
        "SELECT\r\n",
        "  Hours_Studied,\r\n",
        "  Exam_Score,\r\n",
        "  Gender\r\n",
        "FROM catalog_dev.golden.fact_student_performance;"
      ]
    }
  ],
